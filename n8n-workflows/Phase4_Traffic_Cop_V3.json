            "type": "n8n-nodes-base.webhook",
            "typeVersion": 1,
            "position": [
                200,
                450
            ]
        },
        {
            "parameters": {
                "operation": "append",
                "documentId": {
                    "__rl": true,
                    "value": "",
                    "mode": "id"
                },
                "sheetName": {
                    "__rl": true,
                    "value": "",
                    "mode": "name"
                },
                "fieldsUi": {
                    "fieldValues": [
                        {
                            "fieldId": "URL",
                            "fieldValue": "={{ $json.body.url }}"
                        },
                        {
                            "fieldId": "Status",
                            "fieldValue": "Completed"
                        },
                        {
                            "fieldId": "Report",
                            "fieldValue": "={{ $json.body.report }}"
                        },
                        {
                            "fieldId": "Timestamp",
                            "fieldValue": "={{ $now }}"
