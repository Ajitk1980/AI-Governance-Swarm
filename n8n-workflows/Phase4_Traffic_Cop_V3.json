                "options": {
                    "timeout": 1200000
                }
            },
            "id": "2",
            "name": "HTTP Request (Swarm API)",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 4.1,
            "position": [
                420,
                200
            ]
        },
        {
            "parameters": {
                "options": {},
                "respondWith": "text",
                "responseBody": "={{ $json.report }}"
